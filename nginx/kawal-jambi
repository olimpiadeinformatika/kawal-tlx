server {
  root /home/coderbodoh/kawal-tlx/jambi;
  index index.html;
  server_name jambi.olimpiadeinformatika.com;
  location / {
    try_files $uri $uri/ =404;
  }

    listen [::]:443 ssl; # managed by Certbot
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/jambi.olimpiadeinformatika.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/jambi.olimpiadeinformatika.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}

server {
    if ($host = jambi.olimpiadeinformatika.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


  listen 80;
  listen [::]:80;
  server_name jambi.olimpiadeinformatika.com;
    return 404; # managed by Certbot


}